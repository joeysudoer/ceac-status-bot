# 必需的环境变量
# 签证申请信息
LOCATION=your_location
NUMBER=your_case_number
PASSPORT_NUMBER=your_passport_number
SURNAME=your_surname

# 状态 API 服务地址（必需）
STATUS_API_BASE_URL=http://

# 可选的环境变量
# Email 通知配置
FROM=your_email@example.com
TO=recipient@example.com
PASSWORD=your_email_password
SMTP=smtp.gmail.com

# Telegram 通知配置
TG_BOT_TOKEN=your_telegram_bot_token
TG_CHAT_ID=your_telegram_chat_id

# 时区配置（用于通知时间控制）
TIMEZONE=America/New_York

# 活跃时间范围（仅在此时间范围内发送 Refused 状态通知）
# 格式: HH:MM-HH:MM，默认: 00:00-23:59
ACTIVE_HOURS=09:00-22:00

# GitHub Token（如果使用 GitHub Actions）
GH_TOKEN=your_github_token
GITHUB_REPOSITORY=username/repo
